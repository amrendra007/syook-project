{
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jquery": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "no-console": 0,
        "space-before-function-paren": 0,
        "func-names": 0,
        "indent": [
            "error",
            4
        ],
        "consistent-return": 0,
        "no-unused-vars": [
            1,
            {
                "argsIgnorePattern": "res|next|^err"
            }
        ],
        "radix": 0,
        "arrow-body-style": [
            2,
            "as-needed"
        ],
        "no-underscore-dangle": 0
    }
}